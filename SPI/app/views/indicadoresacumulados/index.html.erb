<h1>Indicadoresacumulados</h1>

<table>
  <thead>
    <tr>
      <th>Id ind acumulado</th>
      <th>Segmento</th>
      <th>Isn</th>
      <th>Resp 1 2</th>
      <th>Resp 4 5</th>
      <th>Resolutividad</th>
      <th>Version</th>
      <th>Fecha</th>
      <th>Volumen</th>
      <th colspan="3"></th>
