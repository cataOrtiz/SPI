        <td><%= link_to 'Show', indicadoresacumulado %></td>
        <td><%= link_to 'Edit', edit_indicadoresacumulado_path(indicadoresacumulado) %></td>
        <td><%= link_to 'Destroy', indicadoresacumulado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
