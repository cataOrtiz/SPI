<p id="notice"><%= notice %></p>

<h1>Encuesta</h1>

<table>
  <thead>
    <tr>
      <th>Id encuesta</th>
      <th>Fecha creacion encuesta</th>
      <th>Hora envio encuesta</th>
      <th>Resuelto encuesta</th>
      <th>Version</th>
      <th>Linea</th>
      <th>Motivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encuesta.each do |encuestum| %>
      <tr>
        <td><%= encuestum.id_encuesta %></td>
        <td><%= encuestum.fecha_creacion_encuesta %></td>
        <td><%= encuestum.hora_envio_encuesta %></td>
        <td><%= encuestum.resuelto_encuesta %></td>
        <td><%= encuestum.version %></td>
        <td><%= encuestum.linea %></td>
        <td><%= encuestum.motivo %></td>
        <td><%= link_to 'Show', encuestum %></td>
        <td><%= link_to 'Edit', edit_encuestum_path(encuestum) %></td>
        <td><%= link_to 'Destroy', encuestum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encuestum', new_encuestum_path %>
